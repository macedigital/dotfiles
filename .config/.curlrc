# Change user-agent string to EDGE on Win11
user-agent = "Mozilla/5.0 (Windows NT 11.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/107.0.0.0 Safari/537.36 Edge/107.0.0.0"

# When following a redirect, automatically set the previous URL as referer.
referer = ";auto"

# Wait 30 seconds before timing out.
connect-timeout = 30

# Set https as default protocol
proto-default = https

# Only allow http,https and sftp protocols
proto =-all,http,https,sftp

